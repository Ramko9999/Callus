{
  "expo": {
    "newArchEnabled": true,
    "name": "Callus",
    "slug": "Callus",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ramko9999.callus",
      "icon": {
        "light": "./assets/icon/ios/light.png",
        "dark": "./assets/icon/ios/dark.png",
        "tinted": "./assets/icon/ios/tinted.png"
      },
      "splash": {
        "backgroundColor": "#ffffff",
        "image": "./assets/icon/splash/dark.png",
        "dark": {
          "backgroundColor": "#000000",
          "image": "./assets/icon/splash/light.png"
        }
      }
    },
    "android": {
      "versionCode": 18,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/android/adaptive.png",
        "monochromeImage": "./assets/icon/android/adaptive.png",
        "backgroundColor": "#000000"
      },
      "splash": {
        "backgroundColor": "#ffffff",
        "image": "./assets/icon/splash/dark.png",
        "dark": {
          "backgroundColor": "#000000",
          "image": "./assets/icon/splash/light.png"
        }
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.ramko9999.callus"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "enforceNavigationBarContrast": false
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "sounds": ["./assets/audio/rest_notification_ending.wav"],
          "icon": "./assets/icon/android/notification_icon.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "315bb499-fda3-424e-9f18-e80cd1ddc9b3"
      }
    },
    "owner": "ramko9999"
  }
}
